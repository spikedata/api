{
  "name": "@spikedata/api",
  "version": "1.0.0",
  "description": "Spike API library helper",
  "main": "src/main.js",
  "module": "dist/spike-api.esm.mjs",
  "browser": "dist/spike-api.umd.js",
  "author": "ilan@spikedata.co.za",
  "license": "SPIKE",
  "repository": {
    "type": "git",
    "url": "https://github.com/spikedata/api"
  },
  "keywords": [
    "spike",
    "pdf",
    "south africa",
    "bank",
    "statements"
  ],
  "scripts": {
    "test": "mocha test/index --quiet 1",
    "test:verbose": "mocha test/index --quiet 0",
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c rollup.config.js",
    "watch": "rollup -c rollup.config.js -w",
    "fix-format": "npm-run-all fix format",
    "fix": "eslint .",
    "format": "prettier --write 'src/**/*.{js,mjs,jsx,json,css,md}'"
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "4.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "chai": "4.2.0",
    "chai-uuid": "1.0.6",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "mocha": "7.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "rollup": "1.32.0",
    "rollup-plugin-babel": "4.3.3"
  },
  "dependencies": {
    "ajv": "6.12.0",
    "axios": "0.19.2",
    "lodash.merge": "4.6.2"
  },
  "preferGlobal": false,
  "files": [
    "dist/"
  ]
}
